{% verbatim %}
<div class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container">
        
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
           <a class="" href="{{ path('{% endverbatim %}{{ name.underscore }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_default_homepage') }}">
               <img class="navbar-brand-logo pull-left img-rounded-small" src="{{ asset(('media/images/logo.png') | imagine_filter('logotop')) }}" alt="" /> 
            </a>
            <a class="navbar-brand" href="{{ path('{% endverbatim %}{{ name.underscore }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_default_homepage') }}">
                 {{ c33s_core_config.app.name }}
            </a>
           
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
                <li class="dropdown {{ macros.isActive('home') }}"><a href="{{ path('{% endverbatim %}{{ name.underscore }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_default_homepage') }}" >Home</a></li>
                <li class="dropdown {{ macros.isActive('about-us') }}">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Über Uns <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="{{ macros.static('team','ueber-uns') }}">Team</a></li>
                        <li><a href="{{ macros.static('videos','ueber-uns') }}">Videos</a></li>
                    </ul>
                </li>
                {#
                <li class="{{ macros.isActive('information') }} dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Informationen <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="{{ path('{% endverbatim %}{{ name.underscore }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_information_index') }}">Allgemeine Informationen</a></li>
                        <li><a href="{{ path('{% endverbatim %}{{ name.underscore }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_faq_index') }}">Häufig gestellte Fragen</a></li>
                        <li><a href="{{ path('{% endverbatim %}{{ name.underscore }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_terms_index') }}">Fachbegriffe</a></li>
                    </ul>
                </li>
                <li class="{{ macros.isActive('contact') }} dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Kontakt <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="{{ macros.static('kontakt') }}">Kontakt</a></li>
                        <li><a href="{{ macros.static('termine') }}">Termin vereinbaren</a></li>
                        <li><a href="{{ macros.static('anfahrt','kontakt') }}">Anfahrt</a></li>
                        <li><a href="{{ macros.static('impressum') }}">Impressum</a></li>
                    </ul>
                </li>
                #}
                {% if is_granted('ROLE_ADMIN') %}
                    {% if app.environment != 'dev' %}
                        {% set domainExt = '' %}
                    {% else %}
                        {% set domainExt = '.dev' %}
                    {% endif %}
                <li class="admin {{ macros.isActive('admin') }} dropdown">
                    <a href="#" class="admin dropdown-toggle" data-toggle="dropdown">Admin <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="admin"><a class="admin" href="{{path('{% endverbatim %}{{ name.underscore }}{% verbatim %}_admin_default_homepage')}}"><i class="fa fa-external-link"></i> Admin</a></li>
                        <li class="divider"></li>
                        <li><a href="http://example.com{{ domainExt }}">example.com{{ domainExt }}</a></li>
                    </ul>
                </li>
                {% endif %}
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</div>
{% endverbatim %}