{% block content_pre_container %}{% endblock %}
{# <div class="container"> #}
{% endblock %}

{% block content %}{% endblock %}

{% block content_post %}
{# <br></div> <!-- end container --> #}
{% block content_post_container %}
    <div id="toTop" class="text-right lead">
        {# <a href="#"><i class="fa fa-chevron-up"></i> to top</a> #}
        <a href="#"><i class="fa fa-angle-double-up"></i> to top</a>
        {#% if app_debug == true %#}
        {% if is_granted('ROLE_ADMIN') %}
        <span class="visible-xs">xs</span>
        <span class="visible-sm">sm</span>
        <span class="visible-md">md</span>
        <span class="visible-lg">lg</span>
        {% endif %}
    </div>
{% endblock %}
{% endblock %}



{% block footer %}{% include '{% endverbatim %}{{ name.camelcase }}{% verbatim %}{% endverbatim %}{{ bundlename | camelcase }}{% verbatim %}Bundle::footer.html.twig'%}{% endblock %}

{% block upper_javascripts %}
    {% if c33s_core_config.app.use_cdn %}
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    {% else %}
        <!--[if lt IE 9]>
        {% javascripts output='media/generated/js/html5shiv.js' combine=true
            '%kernel.root_dir%/../vendor/afarkas/html5shiv/src/html5shiv.js'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <![endif]-->
        <script src="{{ asset('components/jquery/jquery.min.js') }}"></script>
    {% endif %}
{% endblock %}

{% block javascripts %}
    <!-- scripts -->
    {#{ parent() }#}
    {% if c33s_core_config.app.use_cdn %}
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
    {% else %}
    {% javascripts output='media/generated/js/bootstrap3.js' combine=true
        '@bootstrap3_js'
        %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% endif %}
    {% javascripts output='media/generated/js/app.js' combine=true
        '@{% endverbatim %}{{ name.camelcase }}{% verbatim %}{% endverbatim %}{{ bundlename | camelcase }}{% verbatim %}Bundle/Resources/public/js/app.js'
        %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% javascripts output='media/generated/js/jquery.cookie.js' combine=true
        '@c33sCoreBundle/Resources/non-public/js/jquery.cookie1.4.js'
        %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
        
    {{ render_assets() }}
        
