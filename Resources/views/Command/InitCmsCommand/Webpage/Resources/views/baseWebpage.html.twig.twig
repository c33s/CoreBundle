{% verbatim %}
{% extends "c33sCoreBundle::base.html.twig" %}
{% import "{% endverbatim %}{{ name.camelcase }}{{ bundlename | camelcase }}{% verbatim %}Bundle::macros.html.twig" as macros %}

{% block head %}
    {{ parent() }}
{% endblock head %}
{% block stylesheets %}
    {{ parent() }}

    {% stylesheets filter='cssrewrite,lessphp' output='media/generated/css/webpage.css' combine=true
        '@{% endverbatim %}{{ name.camelcase }}{{ bundlename | camelcase }}{% verbatim %}Bundle/Resources/non-public/less/webpage/webpage.less'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    
    {{ render_assets('css') }}

{# Font-Awesome 4+ does not support IE7 any more #}
{#     <!--[if IE 7]>#}
{#     <link href="css/font-awesome-ie7.css" rel="stylesheet">#}
{#     <![endif]-->#}

    <!--[if gte IE 9]>
    <style type="text/css">
    </style>
    <![endif]-->
{% endblock %}

{% block header %}
    <!-- header -->
    {% include '{% endverbatim %}{{ name.camelcase }}{{ bundlename | camelcase }}{% verbatim %}Bundle::menu.html.twig' %}
{% endblock %}

{% block content_pre %}
    
