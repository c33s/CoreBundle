{% verbatim %}
{% extends "c33sCoreBundle::base.html.twig" %}
{% import "{% endverbatim %}{{ name.camelcase }}{% verbatim %}{% endverbatim %}{{ bundlename | camelcase }}{% verbatim %}Bundle::macros.html.twig" as macros %}

{% block head %}
    {{ parent() }}
{% endblock head %}
{% block stylesheets %}
    {#{ parent() }#}
    {# lessphp,cssrewrite #}
    {% stylesheets filter='css_url_rewrite,lessphp' output='media/generated/css/bootstrap3.css' combine=true
        '@{% endverbatim %}{{ name.camelcase }}{% verbatim %}{% endverbatim %}{{ bundlename | camelcase }}{% verbatim %}Bundle/Resources/non-public/less/bootstrap3c33s.less'
    %}
        <link rel="stylesheet" href="{{ asset_url }}"  media="screen" />
    {% endstylesheets %}
    {% stylesheets filter='css_url_rewrite,lessphp' output='media/generated/css/theme.css' combine=true
        '%kernel.root_dir%/../vendor/fortawesome/font-awesome/css/font-awesome.css'
        '@{% endverbatim %}{{ name.camelcase }}{% verbatim %}{% endverbatim %}{{ bundlename | camelcase }}{% verbatim %}Bundle/Resources/non-public/less/theme-fonts.less'
        '@{% endverbatim %}{{ name.camelcase }}{% verbatim %}{% endverbatim %}{{ bundlename | camelcase }}{% verbatim %}Bundle/Resources/non-public/less/theme.less'
    %}
        <link rel="stylesheet" href="{{ asset_url }}"  media="screen" />
    {% endstylesheets %}

    <!--[if IE 7]>
    <link href="css/font-awesome-ie7.css" rel="stylesheet">
    <![endif]-->

    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!--[if gte IE 9]>
    <style type="text/css">
    </style>
    <![endif]-->
{% endblock %}

{% block header %}
    <!-- header -->
    {% include '{% endverbatim %}{{ name.camelcase }}{% verbatim %}{% endverbatim %}{{ bundlename | camelcase }}{% verbatim %}Bundle::menu.html.twig' %}
{% endblock %}

