{% verbatim %}
{% extends "C33sCoreBundle::admin-base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}

    {% stylesheets filter='cssrewrite,lessphp' output='media/generated/css/{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}.css' combine=true
        '@{% endverbatim %}{{ name.camelcase }}{{ bundlename | camelcase }}{% verbatim %}Bundle/Resources/non-public/less/{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}/{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}.less'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    
    {## avo start #}
    {#% include 'AvocodeFormExtensionsBundle::stylesheets.html.twig' %#}
    
    {% stylesheets filter='cssrewrite' output='media/generated/css/avocodeformextensions.css'
        'bundles/avocodeformextensions/bootstrap-collection/css/bootstrap-collection.css'
        'bundles/avocodeformextensions/collection-upload/css/collection-upload.css'
        'bundles/avocodeformextensions/datepicker/css/datepicker.css'
        'bundles/avocodeformextensions/daterangepicker/css/daterangepicker.css'
        'bundles/avocodeformextensions/double-list/css/double-list.css'
        'bundles/avocodeformextensions/minicolors/css/jquery.minicolors.css'
        'bundles/avocodeformextensions/select2/css/select2.css'
        'bundles/avocodeformextensions/single-upload/css/single-upload.css'
        'bundles/avocodeformextensions/single-upload/css/filetypeicons.css'
        'bundles/avocodeformextensions/timepicker/css/timepicker.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all" />
    {% endstylesheets %}
    
    {## avo end #}
    
    {{ render_assets('css') }}
	
    {% if form is defined %}
        {{ afe_form_stylesheet(form) }}
    {% endif %}
{% endblock %}

{% block header %}
    {% include '{% endverbatim %}{{ name.camelcase }}{{ bundlename | camelcase }}{% verbatim %}Bundle::menu.html.twig' %}
{% endblock %}

{% block content_pre %}
{#     <div class="container-fluid">#}
{#         <div class="row-fluid">#}
{% endblock %}

{% block content %}
    {#{ bootstrap_set_style('horizontal') }#}
    {{ bootstrap_set_style('vertical') }}
    <div class="row">
        <div class="col-md-12 col-lg-10 col-lg-offset-1">
            {% block admingen_body %}{% endblock %}
        </div>
    </div>
{% endblock %}

{% block content_post %}
{#             </div><!--/span-->#}
{#         </div><!--/row-->#}
         <hr>
{% endblock %}

{% block upper_javascripts %}
    {{ parent() }}
    
    {% javascripts output='media/generated/js/jquery-ui.js'
        'media/components/jquery-ui/jquery-ui.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
{% block javascripts %}
    <!-- scripts -->
    {{ parent() }}
    
    {% javascripts output='media/generated/js/{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}.js' combine=true
        '@{% endverbatim %}{{ name.camelcase }}{{ bundlename | camelcase }}{% verbatim %}Bundle/Resources/non-public/js/{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    
    {#% include 'AvocodeFormExtensionsBundle::javascripts.html.twig' %#}
    {% block avocode_form_javascripts %}
        
        {% javascripts output='media/generated/js/avocodeformextensions.blueimp.jquery-file-upload.min.js'
            'bundles/avocodeformextensions/blueimp-javascript-templates/js/tmpl.js'
            'bundles/avocodeformextensions/blueimp-load-image/js/load-image.js'
            'bundles/avocodeformextensions/blueimp-load-image/js/load-image-ios.js'
            'bundles/avocodeformextensions/blueimp-load-image/js/load-image-orientation.js'
            'bundles/avocodeformextensions/blueimp-load-image/js/load-image-meta.js'
            'bundles/avocodeformextensions/blueimp-load-image/js/load-image-exif.js'
            'bundles/avocodeformextensions/blueimp-load-image/js/load-image-exif-map.js'
            'bundles/avocodeformextensions/blueimp-canvas-to-blob/js/canvas-to-blob.js'
            'bundles/avocodeformextensions/blueimp-file-upload/js/jquery.iframe-transport.js'
            'bundles/avocodeformextensions/blueimp-file-upload/js/jquery.fileupload.js'
            'bundles/avocodeformextensions/blueimp-file-upload/js/jquery.fileupload-process.js'
            'bundles/avocodeformextensions/blueimp-file-upload/js/jquery.fileupload-image.js'
            'bundles/avocodeformextensions/blueimp-file-upload/js/jquery.fileupload-validate.js'
            'bundles/avocodeformextensions/blueimp-file-upload/js/jquery.fileupload-ui.js'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
        {% javascripts output='media/generated/js/avocodeformextensions.form-extensions.min.js'
            'bundles/avocodeformextensions/bootstrap-collection/js/bootstrap-collection.js'
            'bundles/avocodeformextensions/collection-upload/js/collection-upload.js'
            'bundles/avocodeformextensions/datepicker/js/datepicker.js'
            'bundles/avocodeformextensions/daterangepicker/js/date.js'
            'bundles/avocodeformextensions/daterangepicker/js/daterangepicker.js'
            'bundles/avocodeformextensions/double-list/js/double-list.js'
            'bundles/avocodeformextensions/knob/js/jquery.knob.js'
            'bundles/avocodeformextensions/minicolors/js/jquery.minicolors.js'
            'bundles/avocodeformextensions/select2/js/select2.js'
            'bundles/avocodeformextensions/single-upload/js/single-upload.js'
            'bundles/avocodeformextensions/timepicker/js/bootstrap-timepicker.js'
            'bundles/avocodeformextensions/elastic/js/jquery.elastic.js'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
        <!-- locale -->
        {% if app.request.locale in ['ca', 'cs', 'da', 'de', 'es', 'et', 'eu', 'fr', 'gl', 'he', 'hr', 'hu', 'is', 'it', 'ja', 'lt', 'lv', 'mk', 'nl', 'no', 'pl', 'pt-BR', 'pt-PT', 'ro', 'ru', 'sk', 'sv', 'tr', 'ua', 'vi', 'zh-CN', 'zh-TW'] %}
        <script type="text/javascript" src="{{ asset('bundles/avocodeformextensions/select2/js/i18n/select2_locale_' ~ app.request.locale ~ '.js') }}"></script>
        {% endif %}
        
        {% if app.request.locale in ['bg', 'ca', 'cs', 'da', 'de', 'el', 'es', 'et', 'fi', 'fr', 'he', 'hr', 'hu', 'id', 'is', 'it', 'ja', 'kr', 'lt', 'lv', 'mk', 'ms', 'nb', 'nl', 'pl', 'pt-BR', 'pt', 'ro', 'rs-latin', 'rs', 'ru', 'sk', 'sl', 'sq', 'sv', 'sw', 'th', 'tr', 'uk', 'zh-CN', 'zh-TW'] %}
        <script type="text/javascript" src="{{ asset('bundles/avocodeformextensions/datepicker/js/locales/bootstrap-datepicker.' ~ app.request.locale ~ '.js') }}"></script>
        {% endif %}
        
    {% endblock avocode_form_javascripts %}
	
    {% if form is defined %}
        {{ afe_form_javascript(form) }}
    {% endif %}
    
    {{ render_assets('js') }}
{% endblock %}
{% endverbatim %}
