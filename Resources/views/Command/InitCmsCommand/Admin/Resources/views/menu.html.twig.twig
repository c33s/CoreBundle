{% verbatim %}
<div class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
           	<a class="" href="{{ path('{% endverbatim %}{{ name|lower }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_default_homepage') }}">
               <img class="navbar-brand-logo pull-left img-rounded-small" src="{{ asset(('media/images/logo.png') | imagine_filter('adminlogotop')) }}" alt="" id="logo" />
            </a>
            <a class="navbar-brand" href="{{ path('{% endverbatim %}{{ name|lower }}{% verbatim %}_{% endverbatim %}{{ bundlename | underscore }}{% verbatim %}_default_homepage') }}">
                 {{ c33s_core_config.app.name_short }} admin
            </a>
        </div>
        <div class="navbar-collapse collapse navbar-scrollspy">
            {% include "C33sMenuBundle::bootstrap_menu_renderer.html.twig" with { 'items': menu_items('admin_top_left'), containerClass: 'nav navbar-nav' } %}
            {% include "C33sMenuBundle::bootstrap_menu_renderer.html.twig" with { 'items': menu_items('admin_top_right'), containerClass: 'nav navbar-nav navbar-right' } %}
        </div>
    </div>
</div>
{% endverbatim %}