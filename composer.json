{
    "name": "c33s/core-bundle",
    "type": "symfony-bundle",
    "description": "everything to quickstart a webpage",
    "authors": [
        {
            "name": "Julian",
            "email": "office@consistency.at",
            "homepage": "http://consistency.at"
        }
    ],
    "autoload": {
        "psr-0": {
            "c33s\\\\CoreBundle": ""
        }
    },
    "target-dir": "c33s/CoreBundle",
    "license": [
        "MIT",
        "GPL"
    ],
    "homepage": "https://github.com/c33s/CoreBundle",
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.c33s.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "propel/propel-bundle": "1.2.*",
        "willdurand/propel-typehintable-behavior": "~1.0",
        "willdurand/propel-publishable-behavior": "~1.0",
        "fortawesome/font-awesome": "2.0.*",
        "twitter/bootstrap": "2.3.*",
        "leafo/lessphp": "0.3.*",
        "c33s/static-page-content-bundle": "1.0.*",
        "friendsofsymfony/user-bundle": "1.3.*"
    },
    "require-dev": {
        "willdurand/faker-bundle": "1.0.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
