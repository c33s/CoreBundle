{
    "name": "c33s/core-bundle",
    "type": "symfony-bundle",
    "description": "everything to quickstart a webpage",
    "homepage": "https://github.com/c33s/CoreBundle",
    "license": [
        "MIT",
        "GPL-3.0"
    ],
    "authors": [
        {
            "name": "Julian",
            "email": "office@consistency.at",
            "homepage": "http://consistency.at"
        }
    ],
    "autoload": {
        "psr-0": {
            "c33s\\CoreBundle": ""
        }
    },
    "target-dir": "c33s/CoreBundle",
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.c33s.net"
        }
    ],
    "bin": [
        "bin/init-symfony"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.3",
        "c33s/static-page-content-bundle": "1.1.*",
        "c33s/simple-content-bundle": "0.9.*",
        "c33s/menu-bundle": "0.9.*",
        "c33s/attachment-bundle": "0.2.*",
        "propel/propel-bundle": "1.2.*",
        "willdurand/propel-typehintable-behavior": "~1.0",
        "willdurand/propel-publishable-behavior": "~1.0",
        "willdurand/propel-eventdispatcher-bundle": "1.0.*",
        "havvg/propel-boolean-extra-behavior": "~1.1",
        "fortawesome/font-awesome": "4.0.*",
        "components/font-awesome": "3.2.*",
        "fkr/cssurlrewrite-bundle": "1.0.*",
        "twitter/bootstrap": "2.3.*",
        "afarkas/html5shiv": "3.7.*",
        "twbs/bootstrap": "3.1.0",
        "oyejorge/less.php": "~1.5",
        "ptachoire/cssembed": "1.0.*",
        "white-october/pagerfanta-bundle": "1.0.*",
        "cedriclombardot/admingenerator-generator-bundle": "0.99.*",
        "avocode/form-extensions-bundle": "0.99.*",
        "yzalis/identicon": "0.99.*",
        "components/jquery": "1.9.*",
        "components/jqueryui": "1.10.*",
        "malsup/form": "3.36",
        "smoya/asset-management-bundle": "1.0.*",
        "jaysalvat/vegas": "0.99.*",
        "mnot/hinclude": "0.9.*",
        "cocur/human-date": "0.1.*",
        "knplabs/knp-gaufrette-bundle": "0.1.*",
        "knplabs/knp-markdown-bundle": "1.3.*",
        "zenstruck/slugify-bundle": "1.1.*",
        "guzzle/guzzle": "3.8.*",
        "suncat/mobile-detect-bundle": "0.9.*",
        "mobiledetect/mobiledetectlib": "2.7.6",
        "friendsofsymfony/user-bundle": "1.3.*",
        "liip/imagine-bundle": "0.18.*",
        "liip/monitor-bundle": "1.0.*",
        "sensiolabs/security-checker": "1.2.*"
    },
    "require-dev": {
        "willdurand/faker-bundle": "1.0.*"
    }
}
